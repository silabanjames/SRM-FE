    "@fullcalendar/timegrid": "^6.1.10",
    "@headlessui/react": "^1.7.18",
    "@mantine/core": "^7.4.1",
    "@reduxjs/toolkit": "^2.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "@tippyjs/react": "^4.2.6",
    "@types/lodash": "^4.14.202",
    "clsx": "^2.0.0",
    "formik": "^2.4.5",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.297.0",
    "mantine-datatable": "^7.4.3",
    "next": "^14.0.4",
